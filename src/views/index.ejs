<%- include('includes/head') %>
<%- include('includes/nav') %>
  
  <div class="container">
    <div class="row">
      <div class="col-lg-2"></div>

      <div class="col-lg-8 my-3">
        <h1 class="text-center">Agenda</h1>
        <p class="text-center lead"> Seus contatos estao abaixos</p>
      
        <% if(contatos.length > 0){ %>
          <div class="responsive-table">
            <table class="table my-3">
                <% contato.forEcah((contatos) => { %>
                    <td><%= contatos.nome></td>
                    <td><%= contatos.sobrenome></td>
                    <td><%= contatos.telefone></td>
                    <td><%= contatos.email></td>
                    <td><a href="/contato/index"><%=contato._id %>Editar</a></td>
                    <td><a class="text-danger" href="/contato/index"><%=contato._id %>Exclur</a></td>
                <% }) %>
        <% }else{ %>
          <p class="text-center lead"> Nao existe nenhum contato</p>
      <%  } %>
        
          </table>
      </div>
      </div>

 
      <div class="col-lg-2"></div>
    </div>
  </div>
  
  <%- include('includes/footer') %>